<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.DeptMapper">
    <select id="findOne" parameterType="Integer" resultType="com.example.demo.entity.Dept">
        select * from department where id = #{id}
    </select>

    <select id="list" resultType="com.example.demo.entity.Dept">
        select * from department
    </select>

    <insert id="save" parameterType="com.example.demo.entity.Dept" >
        insert into department(id,departmentname) values(#{id},#{departmentName})
    </insert>
</mapper>
